{
    "name": "fuzzyfox/composer-version",
    "description": "Adds version management super powers similar to npm version to you project.",
    "type": "composer-plugin",
    "require": {
        "phlak/semver": "^3.2",
        "composer-plugin-api": "^1.1",
        "ext-json": "*"
    },
    "license": "MPL-2.0",
    "homepage": "https://github.com/fuzzyfox/composer-version",
    "keywords": [
        "version",
        "tag",
        "release",
        "composer",
        "plugin"
    ],
    "authors": [
        {
            "name": "William Duyck",
            "email": "fuzzyfox0@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FuzzyFox\\ComposerVersion\\": "src"
        }
    },
    "extra": {
        "class": "FuzzyFox\\ComposerVersion\\Plugin"
    },
    "version": "v0.1.2"
}